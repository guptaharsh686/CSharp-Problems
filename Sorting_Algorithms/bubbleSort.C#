using System;

  class Program
  {
    static void Main(string[] args)
    {
      int[] arr = {2,5,22,34,5,6};
      
        bubbleSort(arr);
      
      for(int i=0;i<arr.Length;i++){
      Console.WriteLine($"arr[{i}] = {arr[i]}");
    }

    public bubbleSort(int[] arr){
    
        currLength = a.Length;
        
        for(int i=0;i < currLength - 1 ; i++)
        {
            for(int j = 0 ; j <= currLength - 1; j++) 
            {
                if(arr[j] > arr[j+1])
                {
                    int temp = arr[j];
                    arr[j] = arr[j+1];
                    arr[j] = temp;
                    
                }
            }
        currLength--;
        }
    }
  }
